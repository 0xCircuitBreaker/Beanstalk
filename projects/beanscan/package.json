    "sass": "1.57.1",
    "swr": "2.0.1",
    "wagmi": "0.10.15"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
